                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        G.Amount, C.Currency, C.Balance, G.TransPeriod AS Period, G.TransDate, G.PostedDate, G.Postedby, C.Description, G.TransPostedDoc, C.AccType, G.TransDoc AS DocNumber, G.TransType, 0.00 AS AccountBalance, 
                         G.TransComment, G.Account, A.Description AS Period, 0.00 AS LineBalance, G.DC, G.TransCompany, B.CompanyName, 0.00 AS AccTypeBalance, AT.InvoiceNumber, AT.InvDueDate, AT.InvPaymentRef, AT.InvPaymentRef2, 
                         AT.Acc,0.00 as OveralBalance,1 as DisplayZero ,'' as PLGroup,0.00 as PLGroupBalance
FROM            GLTransactions AS G INNER JOIN
                         ChartOfAccounts AS C ON C.AccountNumber = G.Account INNER JOIN
                         AccountingPeriods AS A ON A.AccountingPeriod = G.TransPeriod INNER JOIN
                         AccountingCompanies AS B ON B.CompanyRef = G.TransCompany LEFT OUTER JOIN
                             (SELECT        CustAccNumber AS Acc, InvTransType, 0 AS Invoicenumber2, InvoiceNumber, InvDueDate, InvPaymentRef, InvCartegory, '' AS InvPaymentRef2 
                               FROM            CustomerTransactions
                               UNION
                               SELECT        V1.InvVendorAccNumber AS Acc, V1.InvTransType, V1.InvoiceNumber AS invoicenumber2, COALESCE (v2.InvoiceNumber, V1.InvoiceNumber) AS invoicenumber, V1.InvDueDate, V1.InvPaymentRef, 
                                                        V1.InvCartegory, v2.InvPaymentRef AS InvPaymentRef2
                               FROM            VendorInvoices AS V1 LEFT OUTER JOIN
                                                        VendorInvoices AS v2 ON V1.InvoiceNumber = v2.InvPaymentRef) AS AT ON AT.Acc = G.Account AND CONVERT(nvarchar(50), AT.InvoiceNumber) = G.TransDoc AND AT.InvoiceNumber &lt;&gt; AT.Invoicenumber2</CommandText>
